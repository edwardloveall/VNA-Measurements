<div id="drop_wrap">
  <div id="screen_top"></div>
  <div id="drop">
    <form id="file_form" action="/" method="post" enctype="multipart/form-data">
      <div id="file_select_button">
        Select Measurements
        <input id="file_upload" type="file" name="files[]" data-url="/" multiple>
      </div>
      <ul id="files">
      </ul>
      <div class="actions">
        <input id="submit" type="submit" value="Parse!" />
      </div>
    </form>
    <div id="reflection"></div>
  </div>
  <div id="screen_bottom"></div>
</div>